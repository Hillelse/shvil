{
    "name": "Click It",
    "description": "google sheets status copy",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action":{
        "default_title": "Popup and Content Script",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
}
		
		
		
		
	